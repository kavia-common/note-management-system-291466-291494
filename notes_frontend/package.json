{
  "name": "tizen",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "engines": {
    "node": ">=18 <20"
  },
  "browserslist": [
    "defaults",
    "not IE 11",
    "maintained node versions"
  ],
  "packageManager": "npm@10.8.2",
  "scripts": {
    "preinstall": "node -e \"const v=process.versions.node.split('.').map(Number); if(!(v[0]===18)){console.error('This project expects Node 18.x for build. Detected '+process.versions.node); process.exit(0)}\"",
    "dev": "node ./node_modules/vite/bin/vite.js",
    "build": "node ./node_modules/vite/bin/vite.js build",
    "lint": "eslint .",
    "preview": "node ./node_modules/vite/bin/vite.js preview",
    "build:tizen": "node ./node_modules/vite/bin/vite.js build",
    "package:tizen": "cd dist && zip -r ../app.wgt * ../config.xml"
  },
  "dependencies": {
    "react": "^19.1.1",
    "react-dom": "^19.1.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@types/react": "^19.1.16",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "4.3.4",
    "eslint": "^9.36.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.22",
    "globals": "^16.4.0",
    "vite": "5.4.11"
  },
  "overrides": {
    "vite": "5.4.11",
    "@vitejs/plugin-react": "4.3.4"
  },
  "resolutions": {
    "vite": "5.4.11",
    "@vitejs/plugin-react": "4.3.4"
  }
}
